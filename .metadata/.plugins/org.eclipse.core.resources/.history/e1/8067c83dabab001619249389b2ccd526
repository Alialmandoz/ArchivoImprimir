package gestion.Ui;

import javax.swing.table.DefaultTableModel;

import gestion.dao.ClienteDao;

public class Modeloo {
	
	String[] columnNames = {"nombre","apellido","email","telefono"};
	private ClienteDao clietedao = new ClienteDao();
	public int rowCount = clietedao .contarClientes();
	DefaultTableModel datos = new DefaultTableModel(columnNames, rowCount);

	
	
	
	
}
