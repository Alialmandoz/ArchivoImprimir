package ali.almandoz.hibernate.modelo;

import javax.persistence.*;

@Entity
@Table(name = "LIBRO")
public class Libro {
	@Id
	@Column(name = "ID_LIBRO")
	private Long id;
	@Column(name = "TITULO")
	private String titulo;
	
	
	private Autor autor;
	
	public Libro() {
	}

	public Libro(Long id, String titulo) {
		this.id = id;
		this.titulo = titulo;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	@Override
	public String toString() {
		return "Libro [id=" + id + ", titulo=" + titulo + "]";
	}
	
	
}
